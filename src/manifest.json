{
  "manifest_version": 3,
  "name": "Quick Remove",
  "version": "1.0.0",
  "description": "Allows you to quickly remove an HTML element using a context menu.",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.bundle.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.bundle.js"
  }
}
